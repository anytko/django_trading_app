{
    "builds": [
      {
        "src": "trading_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "trading_project/wsgi.py"
      }
    ],
    "rewrites": [
      { "source": "/login", "destination": "/login.html" }
    ]
  }
  