{
    "builds": [
        {
            "src": "trading_project/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.5"
            }
        }
    ],
    "routes": [
        {
            "src": "/",
            "status": 302,
            "headers": {
                "Location": "/accounts/login/"
            }
        },
        {
            "src": "/(.*)",
            "dest": "trading_project/wsgi.py"
        }
    ]
}